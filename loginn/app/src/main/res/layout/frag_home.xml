<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/lightbur"
    tools:context=".HomeFragment"
    android:fitsSystemWindows="true">

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingRight="0dp"
        android:paddingLeft="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/ph_sos"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            android:layout_marginStart="40.59dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="50dp"
            android:layout_marginRight="50dp"
            android:layout_marginBottom="0dp"
            android:clickable="true"
            android:src="@drawable/ic_sos">

        </ImageView>

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:paddingLeft="0dp"
            android:paddingRight="0dp"
           >


            <ImageView
                android:id="@+id/ph_alarm"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="50dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="50dp"
                android:clickable="true"
                android:src="@drawable/ic_alarm">

            </ImageView>

            <ImageView
                android:id="@+id/ph_fakecall"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_marginStart="190dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="50dp"
                android:layout_marginRight="0dp"
                android:layout_marginBottom="50dp"
                android:layout_alignLeft="@+id/ph_alarm"
                android:clickable="true"
                android:src="@drawable/ic_fakecall">

            </ImageView>

            <ImageView
                android:id="@+id/ph_camera"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="170dp"
                android:layout_marginEnd="50dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="50dp"
                android:layout_alignTop="@+id/ph_alarm"
                android:clickable="true"
                android:src="@drawable/ic_cam">

            </ImageView>

            <ImageView
                android:id="@+id/ph_mail"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_marginStart="190dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="170dp"
                android:layout_marginEnd="50dp"
                android:layout_marginRight="0dp"
                android:layout_marginBottom="50dp"
                android:layout_alignLeft="@+id/ph_camera"
                android:layout_alignTop="@+id/ph_fakecall"
                android:clickable="true"
                android:src="@drawable/ic_icons8_whatsapp">

            </ImageView>

        </RelativeLayout>
        <TextView
            android:id="@+id/tv4"
            android:layout_width="1dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textStyle="bold"
            android:ems="30"
            android:background="@color/white"
            android:text=""
            android:visibility="invisible"
            android:textColor="@color/bur"
            />



        <TextView
            android:id="@+id/Cs1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textStyle="bold"
            android:ems="30"
            android:background="@color/white"
            android:text=""
            android:visibility="invisible"
            android:textColor="@color/bur"
            />
        <TextView
            android:id="@+id/Cs2"
            android:layout_width="1dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textStyle="bold"
            android:ems="30"
            android:background="@color/white"
            android:text=""
            android:visibility="invisible"
            android:textColor="@color/bur"
            />

        <TextView
            android:id="@+id/Cs3"
            android:layout_width="1dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="10dp"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textStyle="bold"
            android:ems="30"
            android:background="@color/white"
            android:text=""
            android:visibility="invisible"
            android:textColor="@color/bur"
            />





    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>